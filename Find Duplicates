public class FindDuplicates{
  public int solution(int nums[]){
    int slow=nums[0];
    int fast=nums[0];
    
    while(slow!=fast){
      slow=nums[slow];
      fast=nums[nums[fast];
     }
     
     int aPtr=nums[0];
     int bPtr=nums[slow];
     
     while(aPtr!=bPtr){
      aPtr=nums[aPtr];
      bPtr=nums[bPtr];
     }
     return apTr;
   }
 }
